{
  "name": "gifted-md@v5.0.0",
  "description": "A Multi Device Whatsapp bot.",
  "keywords": [
    "whatsapp bot"
  ],
  "repository": "https://github.com/mouricedevs/gifted-md",
  "stack": "container",
  "env": { 
      "SESSION_ID": {
      "description": "Put Your Session Id",
      "required": true,
      "value": ""
    },
    "MODE": {
      "description": "Put Mode either public or self",
      "required": true,
      "value": "self"
    },
    "AUTO_STATUS_SEEN": {
      "description": "Put true if you want bot to view status",
      "required": false,
      "value": "true"
    },
    "AUTO_READ": {
      "description": "Put true if you want bot to read message",
      "required": false,
      "value": ""
    },
    "AUTO_TYPING": {
      "description": "Put true if you want bot to show typing",
      "required": false,
      "value": ""
    },
    "AUTO_RECORDING": {
      "description": "Put true if you want bot to show recording audio",
      "required": false,
      "value": ""
    },
    "ALWAYS_ONLINE": {
      "description": "Put true if you want bot to show always online",
      "required": false,
      "value": "true"
    },
    "AUTO_BLOCK": {
      "description": "Put true if you want bot to auto block only 212 numbers",
      "required": false,
      "value": ""
    },
    "REJECT_CALL": {
      "description": "Put true if you want to reject call",
      "required": false,
      "value": "true"
    }
    },
    "formation": {
                "web": {
                        "quantity": 1,
                        "size": "Standard-2X"
                }
        },
    "addons": [{ "plan": "heroku-postgresql:essential-1" }],
    "buildpacks": [
    {
      "url": "heroku/nodejs"
    },
    {
      "url": "https://github.com/DuckyTeam/heroku-buildpack-imagemagick.git"
    },
    {
      "url": "https://github.com/jonathanong/heroku-buildpack-ffmpeg-latest"
    },
    {
      "url": "https://github.com/clhuang/heroku-buildpack-webp-binaries.git"
    }
  ]
}

